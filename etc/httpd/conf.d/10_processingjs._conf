# https://gitlab-uat.adelaide.edu.au/adelaidex/course-units/blob/master/Think.Create.Code/processingjs/etc/httpd/conf.d/10_processingjs._conf

# ref: http://thecodeship.com/deployment/deploy-django-apache-virtualenv-and-mod_wsgi/
Alias /Think.Create.Code/robots.txt  /var/www/django/course-units/Think.Create.Code/processingjs/static/robots.txt
Alias /Think.Create.Code/favicon.ico /var/www/django/course-units/Think.Create.Code/processingjs/static/favicon.ico
Alias /Think.Create.Code/static      /var/www/django/course-units/Think.Create.Code/processingjs/static
Alias /Think.Create.Code/js      	 /var/www/django/course-units/Think.Create.Code/processingjs/static/js
AliasMatch ^/Think.Create.Code/?$    /var/www/django/course-units/Think.Create.Code/processingjs/static/index.html
WSGIScriptAlias /Think.Create.Code/gallery /var/www/django/course-units/Think.Create.Code/processingjs/gallery/wsgi.py
<Directory /var/www/django/course-units/Think.Create.Code/processingjs>
  WSGIProcessGroup django-processingjs
  WSGIApplicationGroup %{GLOBAL}
  <Files wsgi.py>
    Order deny,allow
    Allow from all
  </Files>
</Directory>
